<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tutorazadi.interestdestroyerandroid"
    android:versionCode="5"
    android:versionName="1.2.1" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.tutorazadi.interestdestroyerandroid.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.tutorazadi.interestdestroyerandroid.ResultsActivity"
            android:label="@string/title_activity_results" >
        </activity>
        <activity
            android:name="com.tutorazadi.interestdestroyerandroid.Amortization"
            android:label="@string/title_activity_amortization" >
        </activity>
        <activity
            android:name="com.tutorazadi.interestdestroyerandroid.ResultsChartActivity"
            android:label="@string/title_activity_results_chart" >
        </activity>
        <activity android:name="org.achartengine.GraphicalActivity" />
        <activity
            android:name="com.tutorazadi.interestdestroyerandroid.ExtraPaymentsChart"
            android:label="@string/title_activity_extra_payments_chart" >
        </activity>
        <activity
            android:name="com.tutorazadi.interestdestroyerandroid.BarChart"
            android:label="@string/title_activity_bar_chart" >
        </activity>
        <activity
            android:name="com.tutorazadi.interestdestroyerandroid.ExtraPaymentChart"
            android:label="@string/title_activity_extra_payment_chart" >
        </activity>
    </application>

</manifest>